<%- include("./html", {title: "Nike Membership", navbar:"/CSS/navbar.css", signLink:"", footer:"/CSS/footer.css"})%>
    <link nes="" rel="icon" sizes="128x128" href="https://www.nike.com/android-icon-128x128.png">
    <link rel="stylesheet" href="/CSS/join_us.css">
     <script src="https://kit.fontawesome.com/683b4b40e3.js" crossorigin="anonymous"></script>
    <!-- Navbar -->
    <div id="navbar_suraj"></div>

    <!-- Main join us page content -->
    <div id = "membership">
        <p>Nike Membership</p>
    </div>
    <div id = "join_us">
        <img src="https://static.nike.com/a/images/f_auto,cs_srgb/w_1920,c_limit/560879ab-24f8-4013-ac94-6661849cdf3c/image.jpg " alt="">
    </div>
    <div id = "join_us_content">
        <div>Where All Athletes Belong</div>
        <p>Trying something new is never pretty. But it is pretty awesome.
            Because no matter how many times you fall down, you're still coming out
            on top. Trying new things is always a win.
        </p>
        <p>
            Join the Nike community as we Play New. As a Member, you'll
            have access to inspiration, the latest Nike products and a community
            of people who are trying something new, just like you.
        </p>
        <div id = "join_in">
            <button class = "join_us_click">Join Us</button>
            <button class = "sign_in_click">Sign In</button>
        </div>
    </div>

    <div class = "nike_image">
        <p>Don't Miss</p>
        <img src="https://static.nike.com/a/images/f_auto/dpr_2.0,cs_srgb/w_1700,c_limit/7c029d74-8aff-4130-a563-82ee67282c66/nike-membership.jpg " alt="">
    </div>
    <div id = "hello_app_content">
        <h1>YOUR NIKE CONNECTION</h1>
        <P>More sport, more inspiration, more Nike!</P>
        <button>Get Your Great</button>
    </div>
    <div class = "nike_image">
        <p>First Access</p>
        <img src="https://static.nike.com/a/images/f_auto,cs_srgb/w_1920,c_limit/d1242a08-b6b1-4b13-8cf4-544fc9517a5c/nike-membership.jpg " alt="">
    </div>
    <div class = "nike_image">
        <p>Member Rewards</p>
        <img src="https://cf-images.us-east-1.prod.boltdns.net/v1/jit/72451143001/bdd442d9-e5fc-4e3f-8710-962c9b4ef6ff/main/1808x700/2s176ms/match/image.jpg " alt="">
    </div>
    <div class = "nike_image">
        <p>Shopping As It Should Be</p>
        <img src="https://static.nike.com/a/images/f_auto,cs_srgb/w_1536,c_limit/bfc8265d-4193-4f70-8a0c-f9776b197512/nike-membership.jpg " alt="">
    </div>
    <div id = "athletes">
        <img src="https://static.nike.com/a/images/f_auto/dpr_2.0,cs_srgb/w_1700,c_limit/b0ec2a73-dcb3-456b-b2b7-9bb62374ac62/nike-membership.png" alt="">
        <div>
            <button class = "join_us_click" >Join Us</button>
            <button class = "sign_in_click" >Sign-In</button>
        </div>
    </div>
    <div id = "customise_footer"></div>  
</body>
</html>

<script type = "module">
    import {navbar,mobile} from "/Javascript/navbar_html.js"
    let navbar_html =  navbar()
    document.querySelector("#navbar_suraj").innerHTML = navbar_html
    mobile()


    import footer from "/Javascript/footer.js"
    let footer_html = footer()
    document.querySelector("#customise_footer").innerHTML = footer_html  

    document.querySelectorAll(".join_us_click").addEventListener("click",() =>
    {
        window.location.href = "/in/register"
    })
    document.querySelectorAll(".sign_in_click").addEventListener("click",() =>
    {
        window.location.href = "/in/signin"
    })
