<script>
        fetchData()
        async function fetchData(){
        let res=await fetch("https://myfirssstapinodejs.herokuapp.com/kidsShoes")
        data= await res.json()
        appendData(data);
   }
   


    function appendData(arr){
        arr.forEach(function(elem){
            let container=document.getElementById('kid_container2')
            let div=document.createElement('div')
            let img=document.createElement('img')
            img.src=elem.product_image[0]
            let title=document.createElement('p')
            title.innerText=elem.product_name
            let cat=document.createElement('p')
            cat.innerText=elem.product_category
            let price=document.createElement('p')
            price.innerText='â‚¹'+elem.product_price;
            div.append(img,title,cat,price)
            container.append(div);
        })
    }

</script>